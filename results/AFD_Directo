// AFD
digraph {
	rankdir=LR
	start -> A
	start [shape=point]
	B [shape=doublecircle]
	C [shape=doublecircle]
	D [shape=doublecircle]
	A -> B [label="	"]
	A -> B [label="
"]
	A -> B [label=" "]
	A -> C [label="("]
	A -> C [label=")"]
	A -> C [label="*"]
	A -> C [label="+"]
	A -> D [label=a]
	A -> D [label=b]
	A -> D [label=c]
	B -> B [label="	"]
	B -> B [label="
"]
	B -> B [label=" "]
	D -> D [label=0]
	D -> D [label=1]
	D -> D [label=2]
	D -> D [label=a]
	D -> D [label=b]
	D -> D [label=c]
}
