// AFD
digraph {
	rankdir=LR
	start -> A
	start [shape=point]
	B [shape=circle]
	C [shape=circle]
	D [shape=circle]
	E [shape=circle]
	F [shape=circle]
	G [shape=doublecircle]
	A -> B [label=a]
	B -> C [label=a]
	B -> D [label=b]
	B -> E [label=c]
	C -> F [label=b]
	D -> F [label=b]
	E -> F [label=b]
	F -> G [label=c]
}
