// AFD
digraph {
	rankdir=LR
	start -> A
	start [shape=point]
	A [shape=circle style=bold]
	B [shape=circle]
	C [shape=doublecircle]
	D [shape=circle]
	E [shape=doublecircle]
	A -> B [label=a]
	B -> C [label=a]
	B -> D [label=b]
	C -> E [label=b]
	D -> C [label=a]
	D -> D [label=b]
	E -> E [label=b]
}
